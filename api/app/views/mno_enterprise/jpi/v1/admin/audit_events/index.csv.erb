<%- headers = ['Date', 'User', 'User email', 'Organization', 'Organization Name', 'Action', 'Description'] -%>
<%= CSV.generate_line headers -%>
<%- @audit_events.each do |event| -%>
<%= CSV.generate_line([
  event.created_at,
  event.user[:name],
  event.user[:email],
  event.organization_id,
  event.organization[:name],
  event.key,
  event.description
]) -%>
<%- end -%>
